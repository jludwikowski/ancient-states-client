{
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "variable-name": ["allow-pascal-case"],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "interface-name" : [true, "never-prefix"],
    "object-literal-sort-keys": false
  }
}
